/*
	Rename Columns of AssignedTo table with variables: 
	KEYID -> ID, 
	id -> BugId,
 	what -> AssignedTo,
 	when -> Time, 
	who -> WhoId 
	The primary key is ID

	Author:		Marius Hoegger
	Date:		26.11.2018

	stored in Code/RenameAssignedTo.sql	
*/;

ALTER TABLE AssignedTo RENAME TO tempTable;
CREATE TABLE AssignedTo
(
		ID					INTEGER
	,	BugId 			INTEGER
	, AssignedTo 	TEXT
	,	Time				INTEGER	
	,	WhoId				INTEGER
	, PRIMARY KEY	(ID)
)
;
INSERT INTO AssignedTo(ID, BugId, AssignedTo, Time, WhoId)
SELECT KEYID, id, what, "when", who
FROM tempTable;
DROP TABLE tempTable;
